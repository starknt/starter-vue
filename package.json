{
  "name": "starter-vue",
  "version": "0.0.0",
  "packageManager": "pnpm@7.5.2",
  "description": "",
  "author": "",
  "license": "MIT",
  "keywords": [
    "vue",
    "vue3",
    "vite"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "vue-tsc --noEmit",
    "test": "vitest",
    "lint-staged": "lint-staged"
  },
  "dependencies": {
    "@vueuse/core": "^8.9.4",
    "unocss": "^0.44.7",
    "vue": "^3.2.40"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.25.2",
    "@testing-library/vue": "^6.6.1",
    "@types/node": "^18.8.1",
    "@vitejs/plugin-vue": "^3.1.2",
    "@vue/compiler-sfc": "^3.2.40",
    "eslint": "^8.24.0",
    "happy-dom": "^6.0.4",
    "lint-staged": "^13.0.3",
    "pnpm": "^7.13.0",
    "simple-git-hooks": "^2.8.0",
    "typescript": "^4.8.4",
    "unplugin-auto-import": "^0.9.5",
    "unplugin-vue-components": "^0.21.2",
    "vite": "^3.1.4",
    "vitest": "^0.18.1",
    "vue-tsc": "^0.38.9"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*.{ts, js, vue}": [
      "eslint --fix",
      "git add"
    ],
    "*.{json, yml}": [
      "eslint --fix",
      "git add"
    ]
  }
}
